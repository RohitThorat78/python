ch=input("enter a character:")
if(ch=='A' or ch=='a' or ch=='E' or ch=='e' or ch=='I' or ch=='i' or ch=='O' or ch=='o' or ch=='U' or ch=='u'):
    print(ch, "is vowel")
else:
    print(ch,"is a consonant")
    
def square(n):
    sm=0
    for i in range(1,n+1):
        sm=sm+(i*i)
    return sm
n=7
print(square(n))
